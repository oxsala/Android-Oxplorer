<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout
		xmlns:android="http://schemas.android.com/apk/res/android"
         android:orientation="vertical"
         android:layout_width="fill_parent" 
         android:layout_height="fill_parent">
  <RelativeLayout android:layout_width="fill_parent"
    	android:layout_height="52dp"
    	android:background="@drawable/navigation_bar_top">
    	<TextView android:id="@+id/index_doc_btn_back"
    		android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"
	    	android:layout_marginLeft="5dp"
	    	android:text="Back"
	    	android:textStyle="bold"
	    	android:gravity="center_vertical"
	    	android:layout_centerVertical="true"
	    	android:textColor="@xml/btn_back_selector"
	    	android:background="@xml/btn_back_selector">
    	</TextView>
    	<TextView android:id="@+id/albums_activity_txt_title"
    		android:layout_width="wrap_content"
	    	android:layout_height="wrap_content"
	    	android:text="Index Doccument"
	    	android:textColor="@color/color_white"
	    	android:textSize="20dp"
	    	android:textStyle="bold"
	    	android:gravity="center"
	    	android:layout_centerHorizontal="true"
	    	android:layout_centerVertical="true"/>
    </RelativeLayout>           
    <ListView android:id="@+id/android:list"         
        android:layout_width="wrap_content" 
    	android:layout_height="wrap_content"        
        android:layout_weight="1"
        android:stackFromBottom="false"
        android:transcriptMode="normal"/>
     <Gallery 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/examplegallery" 
	android:layout_width="fill_parent"
	android:layout_height="wrap_content" />	   
</LinearLayout>
